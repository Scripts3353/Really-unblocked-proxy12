{
  "rewrites": [
    {
      "source": "/uv/:path*",
      "destination": "/api/uv/:path*"
    },
    {
      "source": "/go/:path*",
      "destination": "https://krunker.io/:path*"
    },
    {
      "source": "/games/:path*",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/uv.js": {
      "maxDuration": 30
    }
  }
}
